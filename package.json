{
  "name": "doshii-partner-node-sdk",
  "version": "6.0.2",
  "author": "Doshii",
  "description": "Module for Doshii partner APIs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
