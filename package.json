    "Doshii SDK",
    "Doshi partner API"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/node": "^16.10.4",
    "@types/ws": "^8.2.0",
    "jest": "^27.2.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "axios": "^0.23.0",
    "jsonwebtoken": "^8.5.1",
    "rimraf": "^3.0.2",
    "tslib": "^2.3.1",
    "ws": "^7.5.5"
  },
  "jest": {
    "testMatch": [
      "**/tests/*.ts"
    ],
    "testPathIgnorePatterns": [
      "node_modules/",
      "dist",
      "tests/sharedSamples.ts"
