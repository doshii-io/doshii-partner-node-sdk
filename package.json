    "@types/jsonwebtoken": "^9.0.7",
    "@types/lodash": "^4.17.9",
    "@types/node": "^20.14.10",
    "@types/ws": "^8.5.12",
    "jest": "^29.7.0",
    "lodash": "^4.17.21",
    "nock": "^13.5.5",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "jsonwebtoken": "^9.0.2",
    "tslib": "^2.7.0",
    "ws": "^8.18.0"
  },
  "jest": {
    "testMatch": [
      "**/tests/*.ts"
    ],
    "testPathIgnorePatterns": [
