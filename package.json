{
  "name": "doshii-sdk",
  "version": "2.0.0",
  "description": "Module for Doshii partner APIs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/doshii-io/doshii-partner-node-sdk.git",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rimraf dist && tsc",
    "test": "jest"
  },
  "keywords": [
    "Doshii",
    "Doshii SDK",
    "Doshi partner API"
  ],
  "author": "Jobin Thayyil",
  "license": "MIT",
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/node": "^15.12.2",
    "@types/ws": "^7.4.4",
    "jest": "^27.0.4",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "jsonwebtoken": "^8.5.1",
    "tslib": "^2.3.0",
    "ws": "^7.4.6"
  },
  "jest": {
    "testMatch": [
      "**/tests/*.ts"
    ],
    "testPathIgnorePatterns": [
      "node_modules/",
      "dist",
      "tests/sharedSamples.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    }
  }
}
