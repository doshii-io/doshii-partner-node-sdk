{
  "name": "doshii-partner-node-sdk",
  "version": "4.0.5",
  "author": "Doshii",
  "description": "Module for Doshii partner APIs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
